{
  "resources": {
    "backend": {
      "type": "project.v0",
      "path": "../MyNetatmo24.Backend/MyNetatmo24.Backend.csproj",
      "env": {
        "OTEL_DOTNET_EXPERIMENTAL_OTLP_EMIT_EXCEPTION_LOG_ATTRIBUTES": "true",
        "OTEL_DOTNET_EXPERIMENTAL_OTLP_EMIT_EVENT_LOG_ATTRIBUTES": "true",
        "OTEL_DOTNET_EXPERIMENTAL_OTLP_RETRY": "in_memory",
        "ASPNETCORE_FORWARDEDHEADERS_ENABLED": "true"
      },
      "bindings": {
        "http": {
          "scheme": "http",
          "protocol": "tcp",
          "transport": "http"
        },
        "https": {
          "scheme": "https",
          "protocol": "tcp",
          "transport": "http"
        }
      }
    },
    "frontend": {
      "type": "dockerfile.v0",
      "path": "../MyNetatmo24.Frontend/Dockerfile",
      "context": "../MyNetatmo24.Frontend",
      "env": {
        "NODE_ENV": "production",
        "services__backend__http__0": "{backend.bindings.http.url}",
        "services__backend__https__0": "{backend.bindings.https.url}",
        "PORT": "{frontend.bindings.http.targetPort}"
      },
      "bindings": {
        "http": {
          "scheme": "http",
          "protocol": "tcp",
          "transport": "http",
          "targetPort": 8000,
          "external": true
        }
      }
    }
  }
}